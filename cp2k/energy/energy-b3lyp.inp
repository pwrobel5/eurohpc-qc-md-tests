&GLOBAL
  PROJECT ec
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &SUBSYS
    &KIND H
      ELEMENT H
      BASIS_SET DZVP-GTH-BLYP
      POTENTIAL GTH-BLYP-q1
    &END KIND
    &KIND C
      ELEMENT C
      BASIS_SET DZVP-GTH-BLYP
      POTENTIAL GTH-BLYP-q4
    &END KIND
    &KIND O
      ELEMENT O
      BASIS_SET DZVP-GTH-BLYP
      POTENTIAL GTH-BLYP-q6
    &END KIND
    &CELL
      ABC 9.300 11.10 4.291
    &END CELL
    &TOPOLOGY
      COORD_FILE_FORMAT XYZ
      COORD_FILE_NAME 0004-ec.xyz
    &END TOPOLOGY
  &END SUBSYS
  
  &DFT
    CHARGE 0
    
    BASIS_SET_FILE_NAME  BASIS_SET
    POTENTIAL_FILE_NAME  GTH_POTENTIALS
    
    &QS
      EPS_DEFAULT 1.0E-10
    &END QS
    
    &MGRID
      NGRIDS 4
      CUTOFF 300
      REL_CUTOFF 60
    &END MGRID
    
    &XC
      &XC_FUNCTIONAL B3LYP
      &END XC_FUNCTIONAL
      &VDW_POTENTIAL
        POTENTIAL_TYPE PAIR_POTENTIAL
        &PAIR_POTENTIAL
          TYPE DFTD3
          PARAMETER_FILE_NAME dftd3.dat
          REFERENCE_FUNCTIONAL B3LYP
          CALCULATE_C9_TERM T
        &END PAIR_POTENTIAL
      &END VDW_POTENTIAL
      
      &HF
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 2.0
          T_C_G_DATA ../t_c_g.dat
        &END INTERACTION_POTENTIAL
      &END HF
    &END XC
    
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.0E-7
      MAX_SCF 300
      &OT T
      &END OT
    &END SCF
    
    &PRINT
      &MOMENTS SILENT
      &END MOMENTS
    &END PRINT
  &END DFT
&END FORCE_EVAL
