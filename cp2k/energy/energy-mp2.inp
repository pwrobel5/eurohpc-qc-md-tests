&GLOBAL
  PROJECT ec
  RUN_TYPE ENERGY
  PRINT_LEVEL LOW
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &SUBSYS
    &KIND H
      ELEMENT H
      BASIS_SET cc-TZV2P-GTH
      POTENTIAL GTH-HF-q1
    &END KIND
    &KIND C
      ELEMENT C
      BASIS_SET cc-TZV2P-GTH
      POTENTIAL GTH-HF-q4
    &END KIND
    &KIND O
      ELEMENT O
      BASIS_SET cc-TZV2P-GTH
      POTENTIAL GTH-HF-q6
    &END KIND
    &CELL
      ABC 9.300 11.10 4.291
    &END CELL
    &TOPOLOGY
      COORD_FILE_FORMAT XYZ
      COORD_FILE_NAME 0004-ec.xyz
    &END TOPOLOGY
  &END SUBSYS
  
  &DFT
    CHARGE 0
    
    BASIS_SET_FILE_NAME  HFX_BASIS
    POTENTIAL_FILE_NAME  HF_POTENTIALS
    
    &QS
      EPS_DEFAULT 1.0E-10
    &END QS
    
    &MGRID
      NGRIDS 4
      CUTOFF 300
      REL_CUTOFF 60
    &END MGRID
    
    &XC
      &XC_FUNCTIONAL NONE
      &END XC_FUNCTIONAL
      
      &HF
        FRACTION 1.0
        &SCREENING
          EPS_SCHWARZ 1.0E-9
        &END SCREENING
        &INTERACTION_POTENTIAL
          POTENTIAL_TYPE TRUNCATED
          CUTOFF_RADIUS 2.0
          T_C_G_DATA ../t_c_g.dat
        &END INTERACTION_POTENTIAL
        &MEMORY
          MAX_MEMORY 2000
        &END MEMORY
      &END HF
      
      &WF_CORRELATION
        &MP2
          METHOD MP2_GPW
        &END MP2

        MEMORY 2000
        GROUP_SIZE 16
      &END WF_CORRELATION
    &END XC
    
    &SCF
      SCF_GUESS ATOMIC
      EPS_SCF 1.0E-7
      MAX_SCF 15000
      &OT T
      &END OT
    &END SCF
    
    &PRINT
      &MOMENTS SILENT
      &END MOMENTS
    &END PRINT
  &END DFT
&END FORCE_EVAL
